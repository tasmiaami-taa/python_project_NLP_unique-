def find_max_min(numbers):
    if not numbers:
        return "List is empty"
    
    max_val = numbers[0]
    min_val = numbers[0]
    
    for num in numbers:
        if num > max_val:
            max_val = num
        if num < min_val:
            min_val = num
    
    return f"Maximum: {max_val}, Minimum: {min_val}"

# Test the function
numbers_list = [3, 1, 4, 1, 5, 9, 2, 6, 5]
print("List:", numbers_list)
print(find_max_min(numbers_list))

output:
List: [3, 1, 4, 1, 5, 9, 2, 6, 5]
Maximum: 9, Minimum: 1
